{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "dynamicImport": true,
            "decorators": true
        },
        "transform": {
            "decoratorVersion": "2022-03",
            "decoratorMetadata": true
        },
        "target": "es2021",
        "loose": false,
        "externalHelpers": true,
        "keepClassNames": false
    },
    "minify": true,
    "module": {
        "type": "es6",
        "strict": false,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    }
}
