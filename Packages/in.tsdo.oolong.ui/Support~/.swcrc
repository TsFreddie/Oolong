{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "dynamicImport": false,
            "decorators": false
        },
        "transform": {
            "react": {
                "pragma": "m",
                "pragmaFrag": "m.Fragment",
                "throwIfNamespace": true,
                "development": false,
                "useBuiltins": true
            }
        },
        "target": "es2021",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": false
    },
    "minify": true,
    "module": {
        "type": "es6",
        "strict": false,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    }
}
