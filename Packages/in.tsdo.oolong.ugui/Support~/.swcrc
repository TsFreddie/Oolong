{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
      "parser": {
          "syntax": "typescript",
          "tsx": true,
          "dynamicImport": false,
          "decorators": true
      },
      "transform": {
          "react": {
              "pragma": "m",
              "pragmaFrag": "m.Fragment",
              "throwIfNamespace": true,
              "development": false,
              "useBuiltins": true
          },
          "decoratorVersion": "2022-03",
          "decoratorMetadata": true
      },
      "target": "es2021",
      "loose": false,
      "externalHelpers": true,
      "keepClassNames": false
  },
  "minify": true,
  "module": {
      "type": "es6",
      "strict": false,
      "strictMode": true,
      "lazy": false,
      "noInterop": false
  }
}
